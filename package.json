{
  "name": "onecms",
  "private": true,
  "version": "0.1.0",
  "description": "CMS for DKT ecommerce platform",
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "-- FORMAT & LINT --": "----------",
    "format:check": "npx prettier . --config ./common/configs/.prettierrc.json --ignore-path ./common/configs/.prettierignore --check",
    "format:fix": "npx prettier . --config ./common/configs/.prettierrc.json --ignore-path ./common/configs/.prettierignore --write",
    "lint:check": "eslint . --config ./common/configs/eslint.config.js",
    "lint:fix": "eslint . --config ./common/configs/eslint.config.js --fix",
    "-- CI --": "----------",
    "release:create": "npx semantic-release -e ./common/configs/release.config.js",
    "-- OTHERS --": "----------",
    "prepare": "husky"
  },
  "devDependencies": {
    "@semantic-release/changelog": "~6.0.3",
    "@semantic-release/git": "~10.0.1",
    "@semantic-release/github": "~10.0.3",
    "eslint": "~9.1.1",
    "husky": "~9.0.11",
    "prettier": "~3.2.5",
    "semantic-release": "~23.0.8"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "4.23.1",
    "@strapi/plugin-i18n": "4.23.1",
    "@strapi/plugin-users-permissions": "4.23.1",
    "@strapi/strapi": "4.23.1",
    "mysql": "2.18.1",
    "react": "~18.0.0",
    "react-dom": "~18.0.0",
    "react-router-dom": "5.3.4",
    "styled-components": "5.3.3"
  },
  "author": {
    "name": "CorentinTrt"
  },
